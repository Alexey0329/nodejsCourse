{
  "name": "node-course",
  "version": "1.0.0",
  "description": "learning nodejs",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --",
    "test": "jest",
    "test:unit": "jest --testPathPattern=src/test/unit/",
    "test:integration": "jest --testPathPattern=src/test/integration/",
    "test:e2e": "jest --testPathPattern=src/test/e2e/",
    "test:coverage": "jest --coverage",
    "start:server": "pm2 start ts-node ./Module5/server.ts --name ngmp-network-app",
    "stop:server": "pm2 stop ngmp-network-app"
  },
  "author": "AM",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "csvtojson": "^2.0.10",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "pm2": "^5.3.1",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2"
  }
}
